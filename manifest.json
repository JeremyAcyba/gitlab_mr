{
  "name": "Gitlab MR tools",
  "version": "1.3",
  "description": "Gitlab MR tools for Saas",
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "src/popup/index.html",
    "default_icon": "images/image.png"
  },
  "icons": {
    "16": "images/image.png",
    "128": "images/image.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "src/content/content.css"
      ],
      "js": [
        "src/content/async-chrome.js",
        "src/content/helper_api.js",
        "src/content/count_discussions.js",
        "src/content/conditional_display.js",
        "src/content/index.js",
        "src/content_new/index.js"
      ]
    }
  ],
  "manifest_version": 2
}